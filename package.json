{
  "name": "nextjs-cz-tw-template",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "commit": "git add . && cz && git push",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "init:Cli": "npm install -g commitizen && npm install -g conventional-changelog-cli"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "dependencies": {
    "next": "12.1.6",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@types/node": "17.0.9",
    "@types/react": "18.0.9",
    "autoprefixer": "10.4.4",
    "eslint": "8.7.0",
    "eslint-config-next": "12.1.6",
    "postcss": "8.4.13",
    "tailwindcss": "3.0.24",
    "typescript": "4.5.4"
  }
}
